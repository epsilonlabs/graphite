grammar workload.xtext_grammar.Efforts with org.eclipse.xtext.common.Terminals

import "ProjectWorkloadsDSL" 
import "http://www.eclipse.org/emf/2002/Ecore"

Main returns Task:
  {Task}
  efforts+=Effort (NEWLINE efforts+=Effort)*;

Effort returns Effort:
  {Effort}
  (person=[Person])? ':' months=INT;

terminal NEWLINE:
  (' '|'\t')* '\r'? '\n' (' '|'\t')*;